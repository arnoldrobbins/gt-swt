# cmasks --- bit masks for set operations

   longint masks (32)            # bit masks for set operations
   save masks

   data masks /:00000000001, :00000000002, :00000000004, :00000000010,
               :00000000020, :00000000040, :00000000100, :00000000200,
               :00000000400, :00000001000, :00000002000, :00000004000,
               :00000010000, :00000020000, :00000040000, :00000100000,
               :00000200000, :00000400000, :00001000000, :00002000000,
               :00004000000, :00010000000, :00020000000, :00040000000,
               :00100000000, :00200000000, :00400000000, :01000000000,
               :02000000000, :04000000000, :10000000000, :20000000000/
